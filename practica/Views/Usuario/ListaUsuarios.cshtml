@{
    Layout = "~/Views/Shared/usuarioLayout.cshtml";
}

<h1> Esta pagina tendrá la lista de usuarios</h1>

<table class="table">
    <thead>
        <th>Nombre</th>
        <th>Usuario</th>
        <th>Contraseña</th>
    </thead>
    <tbody>


        @("hola mundo")
        @for (int i = 0; i < @ViewBag.usuarios.Count; i++)
        {
            if (i != 0)
            {
                <tr>
                    <td>@ViewBag.usuarios[i].name</td>
                    <td>@ViewBag.usuarios[i].user</td>
                    <td>@ViewBag.usuarios[i].pass</td>
                </tr>
            }

        }

    </tbody>
</table>